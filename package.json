        "@react-router/serve": "7.13.0",
        "@tailwindcss/vite": "^4.1.11",
        "isbot": "^5.1.17",
        "log-update": "^6.1.0",
        "log4js": "^6.9.1",
        "msw": "^2.6.2",
        "novari-frontend-components": "6.0.0",
